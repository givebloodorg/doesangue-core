{
    "name": "doesangueorg/giveblood-core",
    "type": "project",
    "description": "Online platform that helps you to found donors.",
    "keywords": [
        "social",
        "api",
        "network"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "artesaos/migrator": "^2.0",
        "barryvdh/laravel-cors": "^0.11.3",
        "fideloper/proxy": "^4.0",
        "folklore/graphql": "^1.1",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "ramsey/uuid": "^3.8",
        "tymon/jwt-auth": "1.0.*"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "GiveBlood\\": "app/",
            "Modules\\": "app/Modules"
        },
        "classmap": [
            "app/Modules/Users/Database/Seeders",
            "app/Modules/Campaign/Database/Seeders",
            "app/Modules/Blood/Database/Seeders",
            "app/Modules/Bank/Database/Seeders"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
